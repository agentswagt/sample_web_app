a {
    font-size: 50px;
    text-decoration: none;
    font-family: 'Shadows Into Light';
    margin-left: 50px;
    font-weight: bolder;

    border: solid pink;
    color: white;
}
a:hover {
    color: pink;
    font-size: 60px;
    font-weight: bolder;
    border: solid red 1px;
    background-color: green;    
}

hr {
    color: red;
    
}
